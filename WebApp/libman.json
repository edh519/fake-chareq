{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "datatables.net@1.10.22",
      "destination": "wwwroot/datatables.net/"
    },
    {
      "library": "datatables.net-bs4@1.10.22",
      "destination": "wwwroot/datatables.net/datatables.net-bs4/"
    },
    {
      "library": "font-awesome@5.15.1",
      "destination": "wwwroot/font-awesome/"
    },
    {
      "library": "tinymce@7.0.1",
      "destination": "wwwroot/lib/tinymce/"
    },
    {
      "library": "sweetalert2@11.12.2",
      "destination": "wwwroot/lib/sweetalert2/",
      "files": [
        "sweetalert2.min.js",
        "sweetalert2.min.css"
      ]
    },
    {
      "library": "select2@4.1.0-rc.0",
      "destination": "wwwroot/lib/select2/"
    },
    {
      "library": "select2-bootstrap-5-theme@1.3.0",
      "destination": "wwwroot/lib/select2-bs5-theme"
    }
  ]
}